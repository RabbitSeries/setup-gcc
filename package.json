{
  "name": "setup-gcc",
  "version": "1.0.0",
  "description": "github actions for gcc setup",
  "main": "dist/index.cjs",
  "scripts": {
    "build:config": "esbuild ./esbuild.ts --outfile=./dist/esbuild.js --bundle --external:esbuild --external:fast-glob --format=esm --platform=node --target=esnext",
    "build:build": "node ./dist/esbuild.js",
    "build": "run-s build:config build:build",
    "test": "npm run build && node ./dist/index.cjs"
  },
  "type": "module",
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/exec": "1.1.1",
    "@actions/github": "6.0.1"
  },
  "keywords": [
    "github-actions"
  ],
  "author": "rabbit",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@types/node": "^24.3.0",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}